{
  "name": "laterpay/revenue-generator",
  "description": "Monetize your blog and content with Laterpay's Revenue Generator",
  "homepage": "https://github.com/laterpay/revenue-generator/",
  "type": "wordpress-plugin",
  "version": "1.3.1",
  "license": "MIT",
  "require-dev": {
    "automattic/vipwpcs": "^2.0.0",
    "wp-coding-standards/wpcs": "*",
    "dealerdirect/phpcodesniffer-composer-installer": "*",
    "phpcompatibility/phpcompatibility-wp": "*"
  },
  "scripts": {
    "phpcs": "\"./vendor/bin/phpcs\"",
    "phpcbf": "\"./vendor/bin/phpcbf\"",
    "phpcompat": "\"./vendor/bin/phpcs\" -p --standard=PHPCompatibilityWP inc templates revenue-generator.php --runtime-set testVersion 5.6- --extensions=php"
  }
}
