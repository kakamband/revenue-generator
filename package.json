{
	"name": "revenue-generator",
	"version": "0.1.0",
	"homepage": "https://github.com/laterpay/revenue-generator/",
	"description": "Monetize your blog and content with LaterPay's Revenue Generator.",
	"author": "laterpay",
	"license": "MIT",
	"scripts": {
		"format:js": "wp-scripts format-js assets/src",
		"lint:js": "npm run lint:js:src",
		"lint:js:src": "wp-scripts lint-js assets/src",
		"lint:js:fix": "wp-scripts lint-js assets/src --fix",
		"dev": "NODE_ENV=development webpack --mode=development --watch",
		"build": "NODE_ENV=production webpack --mode=production",
		"language:make-po": "wp i18n make-pot . --exclude=\"build\" languages/revenue-generator.po",
		"language": "run-s \"language:*\"",
		"zip": "wp dist-archive . revenue-generator.zip"
	},
	"devDependencies": {
		"@babel/core": "^7.4.3",
		"@babel/preset-env": "^7.4.3",
		"@wordpress/scripts": "^7.0.1",
		"autoprefixer": "^9.7.4",
		"babel-loader": "^8.0.5",
		"copy-webpack-plugin": "^5.1.1",
		"css-loader": "^3.0.0",
		"cssnano": "^4.1.10",
		"file-loader": "^4.0.0",
		"imagemin-webpack-plugin": "^2.4.2",
		"mini-css-extract-plugin": "^0.7.0",
		"node-sass": "^4.12.0",
		"npm-run-all": "^4.1.5",
		"postcss-loader": "^3.0.0",
		"sass-loader": "^7.1.0",
		"url-loader": "^3.0.0",
		"webpack": "^4.30.0",
		"webpack-cli": "^3.3.9",
		"webpackbar": "^4.0.0"
	}
}
