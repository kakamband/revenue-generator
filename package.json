{
	"name": "revenue-generator",
	"version": "1.3.0",
	"homepage": "https://github.com/laterpay/revenue-generator/",
	"title": "Revenue Generator",
	"description": "Monetize your blog and content with Laterpay's Revenue Generator.",
	"author": "laterpay",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/laterpay/revenue-generator.git"
	},
	"scripts": {
		"format:js": "wp-scripts format-js assets/src/js",
		"lint:js": "npm run lint:js:src",
		"lint:js:src": "wp-scripts lint-js assets/src/js",
		"lint:js:fix": "wp-scripts lint-js assets/src --fix",
		"lint:style": "stylelint assets/src/scss/**/*.scss assets/src/scss/*.scss --syntax scss",
		"lint:style:fix": "stylelint assets/src/scss/**/*.scss assets/src/scss/*.scss --syntax scss --fix",
		"dev": "NODE_ENV=development webpack --mode=development --watch",
		"env:start": "./bin/local-env/start.sh",
		"env:stop": "./bin/local-env/stop.sh",
		"env:reset-site": "./bin/local-env/install-wordpress.sh --reset-site",
		"test:e2e": "cross-env WP_BASE_URL=http://localhost:8890 wp-scripts test-e2e --config=tests/e2e/jest.config.js",
		"test:e2e:help": "npm run test:e2e -- --help",
		"test:e2e:interactive": "npm run test:e2e -- --puppeteer-interactive --puppeteer-slowmo=150",
		"build": "NODE_ENV=production webpack --mode=production",
		"language:make-po": "wp i18n make-pot . --exclude=\"build\" languages/revenue-generator.po --headers='{\"Last-Translator\":\"Kimberly Moore <kmoore@laterpay.net>\"}'",
		"language": "run-s \"language:*\"",
		"packages-update": "wp-scripts packages-update",
		"zip": "npm run build && composer install --no-dev --optimize-autoloader && wp dist-archive . revenue-generator.zip"
	},
	"dependencies": {},
	"devDependencies": {
		"@babel/core": "^7.4.3",
		"@babel/preset-env": "^7.4.3",
		"@jest/test-sequencer": "25.2.7",
		"@wordpress/e2e-test-utils": "4.13.0",
		"@wordpress/i18n": "3.15.0",
		"@wordpress/jest-puppeteer-axe": "1.9.0",
		"@wordpress/scripts": "12.2.1",
		"@wordpress/url": "2.19.0",
		"autoprefixer": "^9.7.4",
		"babel-loader": "^8.0.5",
		"copy-webpack-plugin": "^5.1.1",
		"cross-env": "^7.0.2",
		"css-loader": "^3.0.0",
		"cssnano": "^4.1.10",
		"file-loader": "^4.0.0",
		"filemanager-webpack-plugin": "^2.0.5",
		"imagemin-webpack-plugin": "^2.4.2",
		"jest-silent-reporter": "^0.2.1",
		"mini-css-extract-plugin": "^0.7.0",
		"node-sass": "^4.12.0",
		"npm-run-all": "^4.1.5",
		"postcss-loader": "^3.0.0",
		"prettier": "npm:wp-prettier@1.19.1",
		"sass-loader": "^7.1.0",
		"stylelint": "^13.2.1",
		"stylelint-config-wordpress": "^16.0.0",
		"tippy.js": "^6.0.3",
		"url-loader": "^3.0.0",
		"webpack": "^4.30.0",
		"webpack-cli": "^3.3.9",
		"webpackbar": "^4.0.0"
	}
}
